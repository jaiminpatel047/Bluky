@model UserManagementVM
@{
    ViewData["Title"] = "RoleManagement";
}

<form>
    <div class="form-group">
        <label asp-for="ApplicationUser.Name"></label>
        <input class="form-control" asp-for="ApplicationUser.Name" placeholder="Name">
        <small id="emailHelp" class="form-text text-muted">We'll never share your email with anyone else.</small>
    </div>
    <div class="form-group">
        <label asp-for="ApplicationUser.Company"></label>
        <input class="form-control" asp-for="ApplicationUser.Company" placeholder="Company">
    </div>
    <div class="form-check">
        <input type="checkbox" class="form-check-input" id="exampleCheck1">
        <label class="form-check-label" for="exampleCheck1">Check me out</label>
    </div>
    <button type="submit" class="btn btn-primary">Submit</button>
</form>

